{
  "buildCommand": "yarn build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "yarn install",
  "functions": {
    "api/signal.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
